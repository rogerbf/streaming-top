{
  "name": "streaming-top",
  "version": "0.0.1",
  "description": "wraps top in a readable stream",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo 'no tests'",
    "prebuild": "npm test && rimraf dist",
    "build": "babel --ignore *.test.js --out-dir dist src",
    "prepublish": "npm run build"
  },
  "keywords": ["stream", "top", "util", "ps", "processes"],
  "author": "rogerbf <roger@bergfalk.nu> (http://bergfalk.nu/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "rimraf": "2.5.4"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "object-from-string": "0.0.1",
    "parse-top": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/streaming-top.git"
  },
  "bugs": {
    "url": "https://github.com/rogerbf/streaming-top/issues"
  },
  "homepage": "https://github.com/rogerbf/streaming-top#readme"
}
